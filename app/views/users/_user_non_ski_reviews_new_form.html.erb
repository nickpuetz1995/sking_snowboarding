<!-- Form non_ski_review start -->
<%= form_with(model: @non_ski_review, local: true) do |f| %>
<!-- Form non_ski_review end -->

  <!-- Label and input for non_ski_activity_id start -->
  <div class="form-group">
    <%= f.label :non_ski_activity_id %>

    <!-- Input for non_ski_activity_id start -->
    <%= f.text_field :non_ski_activity_id, class: "form-control" %>
    <!-- Input for non_ski_activity_id end -->
  </div>
  <!-- Label and input for non_ski_activity_id end -->

  <!-- Label and input for user_id start -->
  <!-- Input for user_id start -->
  <%= f.hidden_field :user_id, value: @user.id %>
  <!-- Input for user_id end -->
  <!-- Label and input for user_id end -->

  <!-- Label and input for review start -->
  <div class="form-group">
    <%= f.label :review %>

    <!-- Input for review start -->
    <%= f.text_field :review, class: "form-control" %>
    <!-- Input for review end -->
  </div>
  <!-- Label and input for review end -->

  <!-- Label and input for rating start -->
  <div class="form-group">
    <%= f.label :rating %>

    <!-- Input for rating start -->
    <%= f.text_field :rating, class: "form-control" %>
    <!-- Input for rating end -->
  </div>
  <!-- Label and input for rating end -->

  <%= f.submit "Create non ski review", class: "btn btn-block btn-outline-secondary" %>
<% end %>
